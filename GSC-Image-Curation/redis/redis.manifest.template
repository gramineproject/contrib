# This file have basic set of values defined for graminizing Redis images. This manifest template
# is used by `curation_script.sh` to create the user manifest file that will be passed to GSC.

sgx.nonpie_binary = true
sgx.enclave_size = "1024M"
sgx.thread_num = 8

# This is required to run workload as non-root user, any random number less than 1000 works.
# 1000 is for root.
loader.uid=999
loader.gid=999

# Based on user input the manifest file will automatically be modified after this line
